<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://loadingjava.blogspot.in/" elementFormDefault="qualified" attributeFormDefault="qualified"
       >

    <element name="addRequest">
    	<complexType>
    		<sequence>
    			<element name="operand1" type="decimal"/>
    			<element name="operand2" type="decimal"/>
    		</sequence>
    	</complexType>
    </element>
    <element name="addResponse">
    	<complexType>
    		<sequence>
    			<element name="result" type="decimal"/>
    		</sequence>
    	</complexType>
    </element>

    <element name="subtractRequest">
    	<complexType>
