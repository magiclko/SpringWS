    <element name="subtractResponse">
    	<complexType>
    		<sequence>
    			<element name="result" type="decimal"/>
    		</sequence>
    	</complexType>
    </element>		

    <element name="multiplyRequest">
       	<complexType>
    		<sequence>
    			<element name="operand1" type="decimal"/>
    			<element name="operand2" type="decimal"/>
    		</sequence>
    	</complexType>
    </element>
    <element name="multiplyResponse">
    	<complexType>
    		<sequence>
    			<element name="result" type="decimal"/>
    		</sequence>
    	</complexType>
    </element>

    <element name="divideRequest">
        <complexType>
    		<sequence>
    			<element name="operand1" type="decimal"/>
